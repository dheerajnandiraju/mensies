{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/syla.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.frontend"
    },
    "android": {
      "permissions": [
        "ACTIVITY_RECOGNITION",
          "BODY_SENSORS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#C2DED1"
      },
      "package": "com.anonymous.frontend"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "splash": {
      "image": "./assets/images/syla.png",
      "text": "Syla",
      "resizeMode": "contain",
      "backgroundColor": "#C2DED1"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/syla.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#C2DED1"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
